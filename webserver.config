C Конфигурация веб-сервера
(def max_workers 4);
(def default_ports array(80, 443, 8080));

--[[
Основные настройки сервера
]]
server_name = "my_web_server";
port = 8080;
workers = {max_workers};
allowed_ports = {default_ports};

C Настройки SSL
ssl_enabled = true;
ssl_ports = array(443, 8443);

--[[
Настройки времени ожидания
]]
timeout = 30;
keep_alive = 15;

C Дополнительные настройки
features = array(gzip, cache, https);
